<?xml version="1.0" encoding="utf-8"?>
<!--
This is the main configuration file for IMAP Checker
REQUIRED FIELDS: You must set values for IMAP_Server, IMAP_User, and IMAP_Password. 
-->
<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="IMAP_Checker.My.MySettings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  
  <applicationSettings>
    <IMAP_Checker.My.MySettings>
      
      <setting name="IMAP_server" serializeAs="String">
        <value>your.imap.server</value> <!--Enter your IMAP server here-->
      </setting>
      <setting name="IMAP_user" serializeAs="String">
        <value>username</value> <!--Enter your IMAP username here-->
      </setting>
      <setting name="IMAP_password" serializeAs="String">
        <value>password</value> <!--Enter your IMAP Password here-->
      </setting>
      <setting name="use_ssl" serializeAs="String">
        <value>False</value> <!--Change For SSL Authentication. Allowed values are 'True' or 'False'-->
      </setting>
      <setting name="port" serializeAs="String">
        <value>143</value> <!--Set the Port your IMAP server listens on. Default for non SSL is 143. Default for SSL is 993-->
      </setting>
      <setting name="delete_msg" serializeAs="String">
        <value>False</value> <!--Tells IMAP Checker whether or not you want to delete the message after it is read. Allowed values are 'True' or 'False' -->
      </setting>
      <setting name="debug_level" serializeAs="String">
        <value>0</value> <!--If set to 1, will dispay various message in the console when the program runs. Useful for seeing where the program may be getting hung up-->
      </setting>
      <setting name="from" serializeAs="String">
        <value></value> <!--Specify the from address of the message you want to find. Blank returns all senders-->
      </setting>
      <setting name="subject" serializeAs="String">
        <value></value> <!--Specify the subject of the message you wish to find. Blank ingnores the subject of the message-->
      </setting>
    </IMAP_Checker.My.MySettings>
  </applicationSettings>

</configuration>